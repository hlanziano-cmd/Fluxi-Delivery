{
    "version": 2,
    "buildCommand": "echo 'No build needed - static files'",
    "outputDirectory": ".",
    "rewrites": [
        {
            "source": "/domiciliarios",
            "destination": "/app-domiciliarios.html"
        },
        {
            "source": "/domiciliarios/",
            "destination": "/app-domiciliarios.html"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/assets/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000, immutable"
                }
            ]
        }
    ]
}
